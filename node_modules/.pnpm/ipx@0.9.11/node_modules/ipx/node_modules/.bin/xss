#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../xss@1.0.14/node_modules/xss/bin/xss" "$@"
else
  exec node  "$basedir/../../../../../xss@1.0.14/node_modules/xss/bin/xss" "$@"
fi
